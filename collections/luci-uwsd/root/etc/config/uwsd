config uwsd 'main'
	option tls '0'
	option port '80'
	# 'global', 'http', 'ws', 'ssl' or 'script'
	# empty or absent means all
	list logchannels 'global'
	list logchannels 'ws'
	option loglevel 'warn'

config uwsd 'wss'
	option tls '1'
	option port '443'
	option cert 'wss.crt'
	option key 'wss.key'
	list logchannels 'global'
	list logchannels 'ws'
	# 'debug', 'info', 'warn' or 'err'
	# option loglevel 'debug'
	list tlsvers 'TLSv1.3'

config cert 'defaults'
	option days '397'
	option key_type 'ec'
	option ec_curve 'P-256'
	option bits '2048'
	option country 'ZZ'
	option state 'Somewhere'
	option location 'Unknown'
	option commonname 'OpenWrt'